<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>eidos.ml</PackageId>
    <Product>eidos.ml</Product>
    <Title>eidos.ml</Title>
    <Version>0.1.0-alpha$([System.DateTime]::Now.ToString(`yyMMddHHmm`))</Version>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
    <Authors>Evgeniy Mamchenko</Authors>
    <Description>eidos.ml is a library for machine learning writen in C# and based on MathNet.Numerics.</Description>
    <PackageTags>MachineLearning;ML</PackageTags>
    <NoWarn>1701;1702;1705;NU1701</NoWarn>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>

  <PropertyGroup>
    <WarningsAsErrors />
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MathNet.Numerics" Version="3.19.0" />
    <PackageReference Include="System.Collections.Immutable" Version="1.3.1" />
  </ItemGroup>

</Project>
